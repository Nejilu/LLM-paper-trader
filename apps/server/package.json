{
    "name": "@paper-trading/server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "predev": "pnpm --filter @paper-trading/db prisma:migrate",
        "dev": "tsx watch src/index.ts",
        "prestart": "pnpm --filter @paper-trading/db prisma:migrate",
        "prebuild": "pnpm --filter @paper-trading/db build && pnpm --filter @paper-trading/server-shared build",
        "build": "tsc -p tsconfig.build.json",
        "start": "node dist/index.js",
        "lint": "eslint \"src/**/*.ts\"",
        "test": "vitest run --passWithNoTests"
    },
    "dependencies": {
        "@paper-trading/db": "workspace:*",
        "@paper-trading/server-shared": "workspace:*",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "lru-cache": "^10.2.1",
        "yahoo-finance2": "^2.9.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/node": "^20.12.7",
        "eslint": "^8.57.0",
        "tsx": "^4.15.7",
        "typescript": "^5.6.2",
        "vitest": "^3.2.4"
    }
}
