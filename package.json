{
    "name":  "paper-trading-llm",
    "private":  true,
    "version":  "0.0.0",
    "engines":  {
                    "node":  "^20.19.0 || >=22.12.0"
                },
    "scripts":  {
                    "dev":  "concurrently \"pnpm --filter @paper-trading/server dev\" \"pnpm --filter @paper-trading/web dev\"",
                    "build":  "pnpm -r run build",
                    "lint":  "pnpm -r run lint",
                    "test":  "pnpm -r run test",
                    "postinstall":  "pnpm --filter @paper-trading/db prisma:generate"
                },
    "devDependencies":  {
                            "concurrently":  "^8.2.2",
                            "typescript":  "^5.6.2",
                            "eslint":  "^8.57.0",
                            "@typescript-eslint/eslint-plugin":  "^7.13.0",
                            "@typescript-eslint/parser":  "^7.13.0",
                            "eslint-config-prettier":  "^9.1.0",
                            "prettier":  "^3.3.3",
                            "vitest":  "^3.2.4",
                            "@types/node":  "^20.12.7"
                        },
    "pnpm":  {
                 "packageManager":  "pnpm@9.6.0",
                 "overrides":  {
                                   "esbuild":  "0.25.9",
                                   "vite":  "7.1.9"
                               }
             }
}
